{
  "datacenter": "smartgrid-dc1",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "node_name": "consul-server",
  "server": true,
  "bootstrap_expect": 1,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "ports": {
    "http": 8500,
    "https": 8501,
    "grpc": 8502,
    "dns": 8600
  },
  "acl": {
    "enabled": true,
    "default_policy": "allow",
    "enable_token_persistence": true
  }
}

